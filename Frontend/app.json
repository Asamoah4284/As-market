{
  "expo": {
    "name": "Asarion Marketplace",
    "slug": "asarion-marketplace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
    
      "resizeMode": "contain",
      "backgroundColor": "#d92131"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.asarion.marketplace"
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#d92131"
      },
      "package": "com.asarion.marketplace",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "asarion",
              "host": "reset-password"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "asarion",
              "host": "product"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff",
          "sounds": []
        }
      ]
    ],
    "scheme": "asarion",
    "extra": {
      "eas": {
        "projectId": "987ca341-92d7-4d38-be41-f41cc65182c3"
      },
      "API_URL": "https://unimarket-ikin.onrender.com",
      "PAYSTACK_PUBLIC_KEY": "pk_live_544b2e05faa2e81574c0ff6fb9353f20fd880959"
    }
  }
}

